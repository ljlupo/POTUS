DROP TABLE POTUS CASCADE CONSTRAINTS PURGE;
CREATE TABLE POTUS  
  (POTUS_SEQ  NUMBER, 
   FNAME      VARCHAR2(50),
   MNAME      VARCHAR2(50), 
   LNAME      VARCHAR2(50), 
   START_DT   DATE, 
   END_DT     DATE,
   STATE      VARCHAR2(50), 
   CONSTRAINT  POTUS_PK  PRIMARY KEY (POTUS_SEQ)
  )
 parallel 4
 nologging
;
